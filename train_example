        -Dtables=\"odps://college_corp_dev/tables/universal_transformer_future_two_month_seven_days_sort_predict_freq\" \
        -DentryFile=\"main/train.py\" \
        -DgpuRequired=\"400\"\
        -Dbuckets=\"oss://one-graph/?host=cn-zhangjiakou.oss-internal.aliyun-inc.com&role_arn=acs:ram::1459115138698067:role/yuyang\" \
        -DuserDefinedParameters='--snapshot=${SNAPSHOT} --max_steps=${ITERATION} --checkpoint_dir=${CHECKPOINT_PATH}' \
        "
